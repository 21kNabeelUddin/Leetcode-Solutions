class Solution {
public:
    int majorityElement(vector<int>& nums) 
    {
        //its already mentioned that majority element takes a bit more spave then lenght/2 
        //so sort and return middle
        std::sort(nums.begin(), nums.end());
        int len=nums.size();
        int index=floor(len/2); 
        return nums[index];
    }
};
