class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) 
    {
        int p1,p2;
        p1=nums[0];
        int len=nums.size();

        for(int i=0; i<len-1; i++)
        {
            for(int j=i+1; j<len; j++)
            {
                if(nums[i]+nums[j]==target)
                {
                    return {i,j};
                }
            }
        }
        return {};

    }
};
